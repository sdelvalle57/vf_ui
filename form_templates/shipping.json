{
    "type": "FDA",
    "name": "Shipping",
    "id": "Shipping",
    "commitment": "TRANSFER",
    "trigger": "DISPATCH",
    "events": [
        {
            "type": "ECONOMIC_EVENT",
            "action": "LOAD",
            "id": "LOAD_S_1",
            "role": "INPUT",
            "values": [
                {
                    "id": "RESOURCE_SPECIFICATION_1",
                    "class": "RESOURCE_SPECIFICATION",
                    "field": "Product",
                    "type": "SELECT",
                    "description": "Product being shipped, linked to commodity and variety",
                    "form-required": true,
                    "accept-default": true
                },
                {
                    "id": "ECONOMIC_RESOURCE_1",
                    "class": "ECONOMIC_RESOURCE",
                    "field": "Product Reference",
                    "type": "SELECT",
                    "description": "Product reference number",
                    "form-required": true,
                    "accept-default": true
                },
                {
                    "id": "QUANTITY_1",
                    "class": "QUANTITY",
                    "field": "Quantity",
                    "type": "NUMBER",
                    "description": "Quantity of food being shipped",
                    "form-required": true,
                    "accept-default": false
                },
                {
                    "id": "AGENT_1",
                    "class": "AGENT",
                    "field": "Shipping Company",
                    "type": "SELECT",
                    "description": "Name of the company that cooled the food",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "LOCATION_1",
                    "class": "LOCATION",
                    "field": "Shipping Location",
                    "type": "SELECT",
                    "description": "Name of the location where the food is cooled",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "AGENT_2",
                    "class": "AGENT",
                    "field": "Receiver Company",
                    "type": "SELECT",
                    "description": "Name of the company that receives the food",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "LOCATION_2",
                    "class": "LOCATION",
                    "field": "Receiver Location",
                    "type": "SELECT",
                    "description": "Name of the location where the food is being shipped",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "HAS_POINT_IN_TIME_1",
                    "class": "HAS_POINT_IN_TIME",
                    "field": "Shipping Date",
                    "type": "DATE",
                    "description": "Date when the food is being shipped",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": false
                },
                {
                    "id": "NOTE_1",
                    "class": "NOTE",
                    "field": "Notes",
                    "type": "TEXT",
                    "description": "Process notes",
                    "form-required": false,
                    "accept-default": false
                }
            ],
            "groups": [
                {
                    "name": "Product",
                    "class": "ECONOMIC_RESOURCE",
                    "fields": ["RESOURCE_SPECIFICATION_1", "ECONOMIC_RESOURCE_1", "QUANTITY_1"]
                },
                {
                    "name": "Shipping Location",
                    "class": "LOCATION",
                    "fields": ["AGENT_1, LOCATION_1"]
                },
                {
                    "name": "Receiver Location",
                    "class": "LOCATION",
                    "fields": ["AGENT_2, LOCATION_2"]
                }
            ]
        }
    ]
}