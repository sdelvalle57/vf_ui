{
    "type": "FDA",
    "name": "Receiving",
    "id": "Receiving",
    "fulfills": "Shipping",
    "trigger": "UNLOAD",
    "events": [
        {
            "type": "ECONOMIC_EVENT",
            "action": "ACCEPT",
            "id": "ACCEPT_R_1",
            "role": "INPUT",
            "values": [
                {
                    "id": "RESOURCE_SPECIFICATION_1",
                    "class": "RESOURCE_SPECIFICATION",
                    "field": "Product",
                    "type": "SELECT",
                    "description": "Product being received, linked to commodity and variety",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "QUANTITY_1",
                    "class": "QUANTITY",
                    "field": "Quantity",
                    "type": "NUMBER",
                    "description": "Quantity of food being shipped",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": false
                },
                {
                    "id": "AGENT_1",
                    "class": "AGENT",
                    "field": "Shipping Company",
                    "type": "SELECT",
                    "description": "Name of the company that received the food",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "LOCATION_1",
                    "class": "LOCATION",
                    "field": "Shipping Location",
                    "type": "SELECT",
                    "description": "Name of the location where the food is received",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": true
                },
                {
                    "id": "HAS_POINT_IN_TIME_1",
                    "class": "HAS_POINT_IN_TIME",
                    "field": "Harvest Date",
                    "type": "DATE",
                    "description": "Date when the food is being shipped",
                    "form-required": true,
                    "flow-through": "EXTERNAL",
                    "accept-default": false
                },
                {
                    "id": "NOTE_1",
                    "class": "NOTE",
                    "field": "Notes",
                    "type": "TEXT",
                    "description": "Process notes",
                    "form-required": false,
                    "accept-default": false
                }
            ],
            "groups": [
                {
                    "name": "Product",
                    "class": "RESOURCE_SPECIFICATION",
                    "fields": ["RESOURCE_SPECIFICATION_1", "QUANTITY_1"]
                },
                {
                    "name": "Receiver Location",
                    "class": "LOCATION",
                    "fields": ["AGENT_1, LOCATION_1"]
                }
            ]
        }
    ]
}